;"d0=0u"
"in0=inf1/2"  
"d12=20u"
"l0=1"
# 32 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
"cnst50=(cnst20/360)*sqrt((2*cnst21)/(p40/2000000))"
"p30=1000000.0/(cnst50*4)"
"cnst31= (p30/p1) * (p30/p1)"
"spw40=plw1/cnst31"

"p31=1000000.0/(cnst51*4)"
"cnst32= (p31/p1) * (p31/p1)"
"spw41=plw1/cnst32"
"spw42=spw41"

"p20=p40"
"p21=p41"
"p22=p42"
"acqt0=0"
# 1 "mc_line 47 file C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) Echo-Antiecho
define delay MCWRK
define delay MCREST
define loopcounter ST1CNT
"ST1CNT = trunc(td1 / 2)"
"MCREST = d1 - d1"
"MCWRK = 0.333333*d1"

    dccorr
# 47 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
# 47 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
1 ze 
# 1 "mc_line 47 file C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x exp. def. of mc cmd. after ze"
LBLAV, MCWRK
# 48 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
# 1 "mc_line 48 file C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x exp. start label for mc cmd."
2 MCWRK 
LBLSTS1, MCWRK 
LBLF1, MCWRK
  MCREST
# 49 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
3 d12 pl1:f1

  p1 ph1
  
  if "l0 %2 == 1"
     {  
     50u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
     p16:gp1           
     d16 
     d16 pl0:f1
     ( center (p41:sp41 ph2):f1 (p21:gp11) )
     d16
     p16:gp1
     d16
     50u    
     50u 
     p16:gp2
     d16
     d16 
     ( center (p40:sp40 ph3):f1 (p20:gp10) )
     d16
     p16:gp2
     d16
     50u
     d0   
     50u
     p16:gp3           
     d16
     d16
     ( center (p42:sp42 ph4):f1 (p22:gp12) )
     d16
     p16:gp3
     d16
     50u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
     d0  
     }
  else
     {
     d0   
     50u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
     p16:gp3           
     d16
     d16
     ( center (p42:sp42 ph4):f1 (p22:gp12) )
     d16
     p16:gp3
     d16
     50u
     d0
     50u 
     p16:gp2
     d16
     d16 
     ( center (p40:sp40 ph3):f1 (p20:gp10) )
     d16
     p16:gp2
     d16
     50u
     50u
     p16:gp1           
     d16
     d16
     ( center (p41:sp41 ph2):f1 (p21:gp11) )
     d16
     p16:gp1
     d16
     50u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
     } 
  
  go=2 ph31
# 1 "mc_line 119 file C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x exp. mc cmd. in line"
  MCWRK  wr #0 if #0 zd iu0
  lo to LBLSTS1 times 2
  MCWRK id0
  lo to LBLF1 times ST1CNT
  MCWRK rf #0
  lo to LBLAV times tdav
# 120 "C:/Bruker/TopSpin3.6.1/exp/stan/nmr/lists/pp/user/UoM_2d_psychejresph_ts4x"
exit 
   
ph1= 0 0 0 0 0 0 0 0			; Hard_90		
ph2= 0 0 2 2 0 0 2 2			; sweep-180		
ph3= 0 1 0 1 2 3 2 3			; PSYCHE
ph4= 0 0 0 0 0 0 0 0			; sweep-180
ph31=0 2 0 2 0 2 0 2			; Receiver

;pl0 : zero power (120 dB)
;pl1 : high power (dB) 
;p1  : 90 degree high power pulse
;p16 : duration of CTP gradients (1m)
;p20 : duration of weak gradient during PSYCHE pulse element
;p21 : duration of weak gradient during 1st 180-degree swept-frequency pulse
;p22 : duration of weak gradient during 2nd 180-degree swept-frequency pulse
;p40 : duration of double-chirp PSYCHE pulse element
;p41 : duration of 1st 180-degree swept-frequency pulse
;p42 : duration of 2nd 180-degree swept-frequency pulse
;d0  : incremented delay
;d1  : relaxation delay
;d16 : recovery delay for gradients
;spw40 : RF power of double-chirp PSYCHE pulse element
;spw41 : RF power of 1st 180-degree swept-frequency pulse
;spw42 : RF power of 2nd 180-degree swept-frequency pulse
;spnam40: file name for PSYCHE pulse element
;spnam41: file name for 1st 180-degree swept-frequency pulse
;spnam42: file name for 2nd 180-degree swept-frequency pulse
;gpz1: CTP gradient (35%)
;gpz2: CTP gradient (49%)
;gpz3: CTP gradient (77%)
;gpz10: weak gradient during PSYCHE element (1-3%)
;gpz11: weak gradient during 1st 180-degree chirp (1-3%)
;gpz12: weak gradient during 2nd 180-degree chirp (1-3%)
;gpnam1: SINE.100
;gpnam2: SINE.100
;gpnam3: SINE.100
;gpnam10: RECT.1
;gpnam11: RECT.1
;gpnam12: RECT.1
;cnst20: desired flip angle for PSYCHE pulse element (degree) (normally 10-25) 
;cnst21: bandwidth of each chirp in PSYCHE pulse element (Hz) (normally 10000)
;cnst51: RF amplitude for 180-degree chirp pulses (Hz)
;l0  : loop for N/R cycle
;in0 : 1/(2 * SW) = DW
;nd0 : 2
;td1 : number of t1 increments
;MC2 : EA

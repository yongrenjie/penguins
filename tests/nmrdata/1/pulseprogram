# 14 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60" 2


"acqt0=-p1*1.32/3.1416"
# 1 "mc_line 18 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60 exp. def. part of mc cmd. before ze"
; dimension 1D; AQ_mode 
define delay MCWRK
define delay MCREST
"MCREST = 30m - 30m"
"MCWRK = 0.333333*30m"

    dccorr
# 18 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
# 18 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
1 ze
# 1 "mc_line 18 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60 exp. def. of mc cmd. after ze"
# 19 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
# 1 "mc_line 19 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60 exp. start label for mc cmd."
2 MCWRK  * 2
LBLF0, MCWRK
  MCREST
# 20 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
  d1
  p1*0.66 ph1
  go=2 ph31
# 1 "mc_line 23 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60 exp. mc cmd. in line"
  MCWRK wr #0
  MCWRK zd
  lo to LBLF0 times td0

# 24 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
exit


ph1=0 2 2 0 1 3 3 1
ph31=0 2 2 0 1 3 3 1
# 31 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/user/zg60"
;pl1 : f1 channel - power level for pulse (default)
;p1 : f1 channel -  90 degree high power pulse
;d1 : relaxation delay; 1-5 * T1
;NS: 1 * n, total number of scans: NS * TD0



# 80 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/Delay.incl"
;$Id: Delay.incl,v 1.14.2.1 2013/08/30 09:43:33 ber Exp $
# 21 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P" 2


# 27 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
define delay DC_SEHSQC1
define delay DC_SEHSQC2
define delay DC_SEHSQC3
define delay DC_SEHSQC4
define delay DC_SEHSQC5
define delay DC_SEHSQC6
define delay DC_SEHSQC7
define delay DC_SEHSQC8
define delay DC_SEHSQC9
define delay DC_SEHSQC10
define delay DH_PSYCHE_1
define delay DH_PSYCHE_2
define delay DN_SEHSQC1
define delay DN_SEHSQC2
define delay DN_SEHSQC3
define delay DN_SEHSQC4
define delay DN_SEHSQC5
define delay DN_SEHSQC6
define delay DN_SEHSQC7
"cnst21  = 10000"                      ; PSYCHE bandwidth
"cnst22  = (cnst20/360)*sqrt((2*cnst21)/(p40/2000000))"       ; PSYCHE RF amplitude
"cnst41  = 2*sfo2/sfo1"                ; gradient ratio
"cnst44  = 2*sfo3/sfo1"                ; gradient ratio
"d0      = 3u"                         ; 13C HSQC t1
"d2      = 0.5s/cnst2"                 ; JCOMP
"d4      = 0.25s/cnst2"                ; 13C INEPT
"d11     = 3u"                         ; PSYCHE t1
"d20     = 3u"                         ; 15N HSQC t1/2
"d24     = 0.25s/cnst4"                ; 15N INEPT
"in0     = inf1/2"                     ; 13C HSQC increment
"in11    = (1/cnst38)/2"               ; PSYCHE increment
"in20    = 1000000/(2*cnst40*sfo3)"    ; 15N HSQC increment: cnst40 = SW(15N)
"p2      = p1*2"                       ; 1H hard 180
"p17     = p16*cnst16"                 ; Longer gradients for 15N seHSQC v2
"p22     = p21*2"                      ; 15N hard 180
"spw40   = plw1*(cnst22/(250000/p1))*(cnst22/(250000/p1))"    ; PSYCHE power level
"DC_SEHSQC1 = d4-p14/2"                     ; zz-filter
"DC_SEHSQC2 = d4+p14/2"                     ; zz-filter
"DC_SEHSQC3 = d4-larger(p2,p14)/2"          ; INEPT
"DC_SEHSQC4 = p16+d16+p2+d0*2-4u-p3*2/PI"   ; 13C pre-t1 if editing
"DC_SEHSQC5 = d2-p16-d16+p3*2/PI"           ; 13C editing period
"DC_SEHSQC6 = d2-p2-p3*2/PI"                ; 13C editing period
"DC_SEHSQC7 = p16+d16+p2/2+d0-4u-p3*2/PI"   ; 13C pre-/post-t1 if no editing
"DC_SEHSQC8 = d6-cnst17*p24/2-p19-d16"      ; first spin echo after t1
"DC_SEHSQC9 = d4-larger(p2,p14)/2-p16-d16"  ; second spin echo after t1
"DC_SEHSQC10= p16+d16-p1*0.78+de+8u"        ; final spin echo for refocusing gradient
"DH_PSYCHE_1  = in11/2-p16-d16-50u"
"DH_PSYCHE_2  = (dw*2*cnst50)+d16+50u"
"DN_SEHSQC1 = d24-p22/2"                    ; zz-filter
"DN_SEHSQC2 = d24+p22/2"                    ; zz-filter
"DN_SEHSQC3 = d24-larger(p2,p22)/2"         ; INEPT
"DN_SEHSQC4 = p17+d16+p2/2+d20-4u"          ; 15N pre- and post-t1, if no editing
"DN_SEHSQC5 = d26-larger(p2,p22)/2-p19-d16" ; first spin echo after t1
"DN_SEHSQC6 = d24-larger(p2,p22)/2-p16-d16" ; second spin echo after t1
"DN_SEHSQC7 = p17+d16-p1*0.78+de+8u"        ; final spin echo for refocusing gradient
define list<gradient> GC_SEHSQC={cnst41}
define list<gradient> GN_SEHSQC={cnst44}
"l0      = td1/6"             ; Total number of 13C t1 increments
"l1      = 0"                 ; Running counter of 13C t1 increments
"l2      = 0"                 ; Counter, even for echo, odd for antiecho
"acqt0   = 0"
baseopt_echo

# 1 "mc_line 90 file C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P dc-measurement inserted automatically"

    dccorr
# 90 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
1 ze
2 30m
3 5m
4 50u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0

# 101 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; Cleanup
  4u pl2:f2
  (p3 ph0):f2
  4u pl3:f3
  (p21 ph0):f3
  4u pl1:f1
  p16:gp0
  d16
  (p1 ph0):f1
  4u
  p16:gp0*-1.37
  d16
  (p1 ph1):f1
  4u
  p16:gp0*0.77
  d16
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
  d1 st0
  4u setnmr3|0 setnmr0|34|32|33 ctrlgrad 0


  ; MODULE 1
  ; 15N-1H seHSQC
# 125 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; reverse zz-filter
  (p1 ph0):f1
  DN_SEHSQC1
  (p22 ph0):f3
  (p2 ph0):f1
  DN_SEHSQC2
  (p1 ph0):f1
  DN_SEHSQC1
  (p22 ph0):f3
  (p2 ph0):f1
  DN_SEHSQC2            ; 15N-1H: y,  X-1H: z

  ; forward INEPT
  (p1 ph3):f1
  DN_SEHSQC3
  4u
  (center (p2 ph0):f1 (p22 ph0):f3 )
  4u
  DN_SEHSQC3
  4u
  (p1 ph1):f1
  (p21 ph4):f3

  ; t1 evolution
  4u
  DN_SEHSQC4
  (p22 ph0):f3
  p17:gp2
  d16
  d20
  (p2 ph7):f1
  d20
  p17:gp2
  d16
  (p22 ph0):f3
  4u
  DN_SEHSQC4
# 163 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; reverse INEPT for first component
  (center (p1 ph0):f1 (p21 ph7):f3 )
  p19:gp8
  d16
  DN_SEHSQC5
  (center (p2 ph0):f1 (p22 ph0):f3 )
  DN_SEHSQC5
  p19:gp8
  d16
  (center (p1 ph1):f1 (p21 ph9):f3 )

  ; reverse INEPT for second component
  p16:gp9
  d16
  DN_SEHSQC6
  (center (p2 ph0):f1 (p22 ph0):f3 )
  DN_SEHSQC6
  p16:gp9
  d16
  (p1 ph0):f1

  ; spin echo for refocusing gradient
  DN_SEHSQC7
  (p2 ph0):f1
  4u
  p17:gp2*EA*GN_SEHSQC
  d16 pl16:f3
  4u
  goscnp ph29 cpd3:f3
  50u do:f3
# 194 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; Cleanup
  4u pl1:f1
  4u pl3:f3
  (p21 ph0):f3
  4u
  p16:gp0*1.77
  2m st


  ; MODULE 2
  ; 13C-1H seHSQC version 2
# 206 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; reverse zz-filter
  (p1 ph0):f1
  DC_SEHSQC1
  (p14:sp3 ph0):f2
  (p2 ph0):f1
  DC_SEHSQC2
  (p1 ph0):f1
  DC_SEHSQC1
  (p14:sp3 ph0):f2
  (p2 ph0):f1
  DC_SEHSQC2            ; 13C-1H: y,  12C-1H: z

  ; forward INEPT
  ; with editing there's an extra 180 on 1H, so we need to start
  ; with the opposite sign to make sure that bulk magnetisation is preserved

  (p1 ph1):f1
# 226 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  DC_SEHSQC3
  4u
  (center (p2 ph0):f1 (p14:sp3 ph0):f2 )
  4u
  DC_SEHSQC3 pl2:f2
  4u
  (p1 ph1):f1 (p3 ph5):f2

  ; t1 evolution with optional multiplicity editing

  4u
  DC_SEHSQC4
  (p31:sp18 ph0):f2
  p16:gp4
  d16 pl2:f2
# 242 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  d0
  (p2 ph7):f1
  d0

  p16:gp4
  d16
  DC_SEHSQC5
  (p31:sp18 ph0):f2
  (p2 ph0):f1
  DC_SEHSQC6 pl2:f2
# 268 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"

# 270 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; reverse INEPT for first component
  (center (p1 ph0):f1 (p3 ph7):f2 )
  p19:gp6
  d16
  DC_SEHSQC8
  (center (p2 ph0):f1 (p24:sp7 ph0):f2 )
  DC_SEHSQC8
  p19:gp6
  d16 pl2:f2
  (center (p1 ph1):f1 (p3 ph9):f2 )  ; seHSQC pulse, incremented with EA

  ; reverse INEPT for second component
  p16:gp7
  d16
  DC_SEHSQC9
  (center (p2 ph0):f1 (p14:sp3 ph0):f2 )
  DC_SEHSQC9
  p16:gp7
  d16
  (p1 ph0):f1

  ; spin echo for refocusing gradient
  DC_SEHSQC10
  (p2 ph0):f1
  4u
  p16:gp4*EA*GC_SEHSQC
  d16 pl12:f2
  4u
  goscnp ph30 cpd2:f2   ; acquire 13C HSQC
  50u do:f2
# 301 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; Cleanup
  4u pl1:f1
  4u pl2:f2
  (p3 ph0):f2
  4u
  p16:gp0*2.32
  2m st


  ; MODULE 3
  ; 1H 1D PSYCHE pure shift spectrum
# 313 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  (p1 ph0):f1
  d11
  DH_PSYCHE_1
  50u
  p16:gp17
  d16
  (p2 ph0):f1
  50u
  p16:gp17
  d16
  DH_PSYCHE_1
  p16:gp18
  d16
  10u
  DH_PSYCHE_2
  ( center (p40:sp40 ph14):f1 (p40:gp14) )
  d16
  10u pl1:f1
  p16:gp18
  d16
  50u
  d11
  go=2 ph26

  ; echo/antiecho loop
  "l2 = l2 + 1"
  1m igrad EA
if "cnst37 == 1"
{
  1m id11
}
  1m ip9*2
  30m wr #0 if #0 zd
  lo to 3 times 2

  ; t1 incrementation
  "l1 = l1 + 1"
# 353 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  1m ip5*2
  1m ip30*2

  ; 13C t1 incrementation

# 359 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  1m id0


  ; 1H t1 incrementation
if "cnst37 > 1"
{
  if "(2 * l1) % cnst37 == 0"
  {
  1m id11
  }
}
# 371 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
  ; 15N t1 incrementation


# 376 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
if "l1 % cnst39 == 0"
{
  1m id20
  1m ip4*2
  1m ip29*2
}


  lo to 4 times l0
# 386 "C:/NMR/data/mfgroup/nmr/_JonY/pp/ngn_noah3-Spv2Spv2P"
50u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7

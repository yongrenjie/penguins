# 80 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/Delay.incl"
;$Id: Delay.incl,v 1.14.2.1 2013/08/30 09:43:33 ber Exp $
# 19 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2" 2


# 22 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
"p2=p1*2"
"d4=1s/(cnst2*4)"
"d11=30m"


# 32 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
"d0=3u"

"in0=inf1/2"

# 37 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
"DELTA1=d4-p16-larger(p2,p14)/2-de-8u"
"DELTA2=d4-larger(p2,p14)/2"
"DELTA3=d4-larger(p2,p14)/2-p1*2/PI"


# 44 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
"DELTA=p16+d16+p2+d0*2"


# 48 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
"acqt0=0"
baseopt_echo


# 1 "mc_line 52 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2 exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) Echo-Antiecho
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = trunc(td1 / 2)"
"MCREST = d1 - d1"
"MCWRK = 0.076923*d1"

    dccorr
# 52 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
1 ze
# 1 "mc_line 52 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2 exp. def. of mc cmd. after ze"
LBLAV, MCWRK
      "d0=d0orig + t1loop * in0 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
# 53 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
  d11 pl12:f2
# 1 "mc_line 54 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2 exp. start label for mc cmd."
2 MCWRK  * 6 do:f2
LBLF1, MCWRK  * 6
LBLST1, MCWRK 
  MCREST
# 55 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
3 (p1 ph1)
  DELTA2 pl0:f2
  4u
  (center (p2 ph1) (p14:sp3 ph6):f2 )
  4u
  DELTA2 pl2:f2 setnmr3|0 setnmr0|34|32|33 ctrlgrad 0
  p28 ph1
  4u
  (p1 ph2) (p3 ph3):f2
  d0 
# 69 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
  (p2 ph5)


  d0
  p16:gp1*EA
  d16 pl0:f2
  4u
  (p24:sp7 ph4):f2
  4u
  DELTA pl2:f2
  (ralign (p1 ph1) (p3 ph4):f2 )
  DELTA3 pl0:f2
  (center (p2 ph1) (p14:sp3 ph1):f2 )
  4u
  p16:gp2
  DELTA1 pl12:f2
  4u setnmr3^0 setnmr0^34^32^33 ctrlgrad 7
  go=2 ph31 cpd2:f2
# 1 "mc_line 87 file C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2 exp. mc cmd. in line"
  MCWRK  do:f2 wr #0 if #0 zd 

  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLF1 times 2
  MCWRK 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  "ph1loop=0"
  MCWRK rf #0
  lo to LBLAV times tdav
# 89 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
exit
   
# 92 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
ph1=0 
ph2=1
ph3=0 2
ph4=0 0 0 0 2 2 2 2
ph5=0 0 2 2
ph6=0
ph31=0 2 0 2 2 0 2 0


;pl0 : 0W
;pl1 : f1 channel - power level for pulse (default)
;pl2 : f2 channel - power level for pulse (default)
;pl3 : f3 channel - power level for pulse (default)
;pl12: f2 channel - power level for CPD/BB decoupling
;sp3: f2 channel - shaped pulse 180 degree for inversion
;sp7: f2 channel - shaped pulse 180 degree for refocussing
;p1 : f1 channel -  90 degree high power pulse
;p2 : f1 channel - 180 degree high power pulse
;p3 : f2 channel -  90 degree high power pulse
;p14: f2 channel - 180 degree shaped pulse for inversion
;p16: homospoil/gradient pulse
;p22: f3 channel - 180 degree high power pulse
;p24: f2 channel - 180 degree shaped pulse for refocussing
;p28: f1 channel - trim pulse
;d0 : incremented delay (2D)                         [3 usec]
;d1 : relaxation delay; 1-5 * T1
;d4 : 1/(4J)XH
;d11: delay for disk I/O                             [30 msec]
;d16: delay for homospoil/gradient recovery
;cnst2: = J(XH)
;inf1: 1/SW(X) = 2 * DW(X)
;in0: 1/(2 * SW(X)) = DW(X)
;nd0: 2
;ns: 1 * n
;ds: >= 16
;td1: number of experiments
;FnMODE: echo-antiecho
;cpd2: decoupling according to sequence defined by cpdprg2
;pcpd2: f2 channel - 90 degree pulse for decoupling sequence
# 133 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
;use gradient ratio:	gp 1 : gp 2
;			  80 : 20.1    for C-13
;			  80 :  8.1    for N-15

;for z-only gradients:
;gpz1: 80%
;gpz2: 20.1% for C-13, 8.1% for N-15

;use gradient files:   
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100
# 146 "C:/Bruker/TopSpin3.6.2/exp/stan/nmr/lists/pp/hsqcetgpsp.2"
                                          ;preprocessor-flags-start
;LABEL_CN: for C-13 and N-15 labeled samples start experiment with 
;             option -DLABEL_CN (eda: ZGOPTNS)
                                          ;preprocessor-flags-end


